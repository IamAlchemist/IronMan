script#calendars-template(type="text/x-handlebars-template").
    {{#each this}}
    <div class="row">
        <div class="panel panel-default">
            <div class="panel-heading">
                <h3 class="panel-title" id="calendarName">{{this}}</h3>
            </div>
            <div class="panel-body">
                <div id="calendar_{{@index}}"></div>
            </div>
        </div>
    </div>
    {{/each}}